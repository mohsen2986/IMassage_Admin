<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="text"
            type="java.lang.String"
            />
    </data>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <TextView
        android:text="@{text}"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        />

</FrameLayout>
</layout>